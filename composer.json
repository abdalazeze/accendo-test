{
    "require": {
        "php": "^7.3",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "symplify/monorepo-builder-prefixed": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "packages/student-app/app/",
                "packages/teacher-app/app/"
            ],
            "Database\\Factories\\": [
                "packages/student-app/database/factories/",
                "packages/teacher-app/database/factories/"
            ],
            "Database\\Seeders\\": [
                "packages/student-app/database/seeders/",
                "packages/teacher-app/database/seeders/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": [
                "packages/student-app/tests/",
                "packages/teacher-app/tests/"
            ]
        }
    },
    "extra": {
        "laravel": []
    },
    "replace": {
        "laravel/laravel": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
